<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <Authors>atst</Authors>
    <Version>0.0.1.0</Version>
    <Company />
    <PackageProjectUrl>https://github.com/atst1996/Gouter</PackageProjectUrl>
    <RepositoryUrl>https://github.com/atst1996/Gouter</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Resources\missing_album.png" />
    <None Remove="Resources\missing_music.png" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Resources\missing_album.png" />
    <Resource Include="Resources\missing_music.png" />
    <EmbeddedResource Include="Xaml\Icons.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
    <EmbeddedResource Include="Xaml\Templates\CustomStyles.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
    <EmbeddedResource Include="Xaml\Templates\DataTemplates.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
    <EmbeddedResource Include="Xaml\Themes\General.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CSCore">
      <Version>1.2.1.2</Version>
    </PackageReference>
    <PackageReference Include="Dapper">
      <Version>2.0.35</Version>
    </PackageReference>
    <PackageReference Include="MessagePack">
      <Version>2.1.152</Version>
    </PackageReference>
    <PackageReference Include="Microsoft-WindowsAPICodePack-Shell">
      <Version>1.1.4</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf">
      <Version>1.1.19</Version>
    </PackageReference>
    <PackageReference Include="SqlKata.Execution">
      <Version>2.2.0</Version>
    </PackageReference>
    <PackageReference Include="System.Data.SQLite.Core">
      <Version>1.0.113.1</Version>
    </PackageReference>
    <PackageReference Include="System.Drawing.Common" Version="4.7.0" />
    <PackageReference Include="System.Reflection.Emit">
      <Version>4.7.0</Version>
    </PackageReference>
    <PackageReference Include="System.Reflection.Emit.Lightweight">
      <Version>4.7.0</Version>
    </PackageReference>
    <PackageReference Include="System.Runtime.CompilerServices.Unsafe">
      <Version>4.7.1</Version>
    </PackageReference>
    <PackageReference Include="System.Threading.Tasks.Extensions">
      <Version>4.5.4</Version>
    </PackageReference>
    <PackageReference Include="z440.atl.core">
      <Version>3.6.0</Version>
    </PackageReference>
  </ItemGroup>
</Project>